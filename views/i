	var func =readOnlyFlgId.value;
try
{

	resourceJson= eval(LangResoureId.value);
	recSch=eval(SchemaJsonTextId.value);

	rec=eval(OutJsonTextId.value);


		var commonHeader="var us = new USS(); var USSContainer0 =    us.USSCreateContainer(); var USSHeader    = ''";
		

		
		var vcommonFrameGen =frameGeneration(resourceJson,rec,recSch,
	{
 name       : 'Dashboard'
