function GenJson(obj)
{

	var SchemaJsonTextId=document.getElementById("SchemaJsonTextId");
	var OutJsonTextId=document.getElementById("OutJsonTextId");
	var MultiLang=document.getElementById("MultiLang"); 
try
{
	schemaJson=eval(SchemaJsonTextId.value);



OutJsonTextId.value=	frameGenerationJson(schemaJson,
	{
 name       : 'Dashboard'
,label      : 'Dashboard'
,task       : 'NONE'
,desc       : ''
,htmlType   : 'PARENT' 
,entitle    : 'READONLY' 
,enttlname  : ''
,mndf       : 'N'
,dataType   : 'CONTAINER'
,cclass     : 'ctable'
,validate   : ''
,parent   : '/'
,dflt       : ''
,min        : '0'
,max        : '20'
,tips       : ''
,onkeyup    : 'onKeyUp(this);'
,onchange   : 'onChange(this);'
,onkeydown  : 'onKeyDown(this);'
,onkeypress : 'onKeyPress(this);'
,onclick    : 'onClick(this);'
,listVal    : '||A|A-ADD|M|M-MODIFY|I|I-INQURY|C|C-CANCEL|V|V-VERIFY'
,help       : 'N'
,helpLink   : 'helpload'
,xml        : 'N'
,Xpath      : '/'
,childs     : schemaJson
} 
);

}
catch(e)
{
	alert("GenJson Exep:" +e);
}


}


function GenFrame(obj)
{

